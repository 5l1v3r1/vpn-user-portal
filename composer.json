{
    "authors": [
        {
            "email": "fkooman@tuxed.net",
            "name": "Fran\u00e7ois Kooman",
            "role": "Developer"
        }
    ],
    "autoload": {
        "files": [
            "src/sodium_compat.php"
        ],
        "psr-4": {
            "SURFnet\\VPN\\Portal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SURFnet\\VPN\\Portal\\Tests\\": "tests/"
        }
    },
    "description": "VPN User Portal",
    "license": "AGPL-3.0-or-later",
    "minimum-stability": "dev",
    "name": "eduvpn/vpn-user-portal",
    "prefer-stable": true,
    "repositories": [
        {
            "no-api": true,
            "type": "vcs",
            "url": "https://github.com/eduvpn/vpn-lib-common"
        },
        {
            "type": "vcs",
            "url": "https://git.tuxed.net/fkooman/php-oauth2-server"
        },
        {
            "type": "vcs",
            "url": "https://git.tuxed.net/fkooman/php-sqlite-migrate"
        },
        {
            "type": "vcs",
            "url": "https://git.tuxed.net/fkooman/php-saml-sp"
        }
    ],
    "require": {
        "bacon/bacon-qr-code": "^1.0",
        "eduvpn/common": "dev-master",
        "ext-date": "*",
        "ext-json": "*",
        "ext-pcre": "*",
        "ext-pdo": "*",
        "ext-spl": "*",
        "fkooman/oauth2-server": "dev-master",
        "fkooman/secookie": "^2",
        "fkooman/sqlite-migrate": "^0",
        "paragonie/constant_time_encoding": "^1|^2",
        "paragonie/random_compat": "^1|^2",
        "fkooman/php-saml-sp": "dev-master",
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35|^5|^6|^7"
    },
    "suggest": {
        "ext-libsodium": "PHP < 7.2 sodium implementation",
        "ext-sodium": "PHP >= 7.2 sodium implementation"
    },
    "support": {
        "email": "eduvpn@surfnet.nl",
        "source": "https://github.com/eduvpn/vpn-user-portal"
    },
    "type": "project"
}
